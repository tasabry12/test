package com.venere.ace.utility;

/**
 *
 * @author quatrini
 */
public class FolderManager {

   public static  String ScenariousFolder;
   public static  String MetaplansFolder;
   public static  String TestCaseFolder;
   public static  String TestFolder;
   public static  String PrintScreenFolder;
   public static  String JsActionFolder;

   public String getTestFolder() {
      return TestFolder;
   }
   
    public String getPrintScreenFolder() {
        return PrintScreenFolder;
    }

    public void setPrintScreenFolder(String PrintScreenFolder) {
        FolderManager.PrintScreenFolder = PrintScreenFolder;
    }


   public void setTestFolder(String TestFolder) {
      FolderManager.TestFolder = TestFolder;
   }
 
   public String getMetaplansFolder() {
      return MetaplansFolder;
   }

   public void setMetaplansFolder(String sFolderMetaplans) {
      FolderManager.MetaplansFolder = sFolderMetaplans;
   }

   public String getScenariousFolder() {
      return ScenariousFolder;
   }

   public void setScenariousFolder(String sFolderScenarious) {
      FolderManager.ScenariousFolder = sFolderScenarious;
   }

   public String getTestCaseFolder() {
      return TestCaseFolder;
   }

   public void setTestCaseFolder(String sFolderTest) {
      FolderManager.TestCaseFolder = sFolderTest;
   }
   
   
   public String getJsActionFolder() {
      return JsActionFolder;
   }

   public void setJsActionFolder(String jsActionFolder) {
      FolderManager.JsActionFolder = jsActionFolder;
   }


}
