package com.venere.ace.dtos;

import java.util.List;

/**
 *
 * @author cquatrini
 */
public class DataAlternativeResponseDTO {
   
   private List<DataHotelDTO> oAlternativeDataHotels;
   private DataHotelDTO oAlternativeHotel;
   private int iAlternativeNumber;

   public int getAlternativeNumber() {
      return iAlternativeNumber;
   }

   public void setAlternativeNumber(int iAlternativeNumber) {
      this.iAlternativeNumber = iAlternativeNumber;
   }

   public DataHotelDTO getAlternativeHotel() {
      return oAlternativeHotel;
   }

   public void setAlternativeHotel(DataHotelDTO alternativeHotel) {
      this.oAlternativeHotel = alternativeHotel;
   }

   public List<DataHotelDTO> getAlternativeDataHotels() {
      return oAlternativeDataHotels;
   }

   public void setAlternativeDataHotels(List<DataHotelDTO> oAlternativeDataHotels) {
      this.oAlternativeDataHotels = oAlternativeDataHotels;
   }

}
